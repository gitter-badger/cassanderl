{application, cassanderl, [
  {description, "Cassandra client"},
  {vsn, "0.4.1"},
  {registered, []},
  {applications, [
    kernel,
    stdlib,
    thrift,
    dispcount,
    cassandra_thrift
  ]},
  {mod, { cassanderl_app, []}},
  {env, [
    {hostname, "127.0.0.1"},
    {port, 9160},
    {default_keyspace, "keyspace1"},
    {worker_pool_size, 30}
  ]}
]}.
